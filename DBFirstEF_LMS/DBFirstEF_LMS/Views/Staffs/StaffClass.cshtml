@model IEnumerable<DBFirstEF_LMS.Models.Assignment>

@{
    ViewBag.Title = "StaffClass";
}
<h2></h2>
@ViewBag.Message
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.assignment_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.assignment_due_dt)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.assignment_open_dt)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Section.Course.course_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Section.Course.course_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Section.section_id)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.assignment_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.assignment_due_dt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.assignment_open_dt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Section.Course.course_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Section.Course.course_id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Section.section_id)
            </td>
            <td>
                <a href="@Url.Action("Edit", "Assignments", new { id = item.assignment_id })">Edit</a> |
                @*@Html.ActionLink("Details", "Details", new { id = item.assignment_id }) |*@
                <a href="@Url.Action("Delete", "Assignments", new { id = item.assignment_id })">Edit</a>
            </td>
        </tr>
    }

</table>
